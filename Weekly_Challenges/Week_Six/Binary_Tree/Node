#include <iostream>
using namespace std;
#ifndef NODEHEADERDEF
#define NODEHEADERDEF

struct node {
    int index;
    double matData;
    struct node* left;
    struct node* right;
    node(int location, double val) {
        index = location;
        matData = val;
        left = NULL;
        right = NULL;
    };
};

struct quadnode {
    int i, j;
    double matData;
    struct quadnode* north;
    struct quadnode* south;
    struct quadnode* east;
    struct quadnode* west;
    quadnode(int row, int col, double num) {
        i = row;
        j = col;
        matData = num;
        north = NULL;
        south = NULL;
        east = NULL;
        west = NULL;
    };

    quadnode() = default;
};

#endif

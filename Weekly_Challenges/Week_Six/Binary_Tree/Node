#include <iostream>
using namespace std;
#ifndef NODEHEADERDEF
#define NODEHEADERDEF

struct node {
    double matData;
    struct node* left;
    struct node* right;
    node(double val) {
        matData = val;
        left = NULL;
        right = NULL;
    };
};

struct quadnode {
    double matData;
    struct quadnode* north;
    struct quadnode* south;
    struct quadnode* east;
    struct quadnode* west;
    quadnode(double val) {
        matData = val;
        north = NULL;
        south = NULL;
        east = NULL;
        west = NULL;
    };
};

#endif

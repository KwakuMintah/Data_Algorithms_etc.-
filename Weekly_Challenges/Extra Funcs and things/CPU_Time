Method One:
Might need #include <cstdio> too
#include <ctime>

int main() {
    std::clock_t start;
    double duration;

    start = std::clock();

    call_func();

    duration = ( std::clock() - start ) / (double) CLOCKS_PER_SEC;

    std::cout<<"printf: "<< duration <<'\n';
}

Method Two:

#include <bits/stdc++.h>  ?

time_t start, end;
time(&start);
ios_base::sync_with_stdio(false); ?
call_func();
time(&end);
double time_taken = end - start;
cout << fixed << time_taken << setprecision(5);

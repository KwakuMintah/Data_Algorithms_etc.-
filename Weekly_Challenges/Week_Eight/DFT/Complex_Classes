#include <iostream>
using namespace std;
#ifndef COMPLEXHEADERDEF
#define COMPLEXHEADERDEF

struct complexInt {
	int real;
	int imag;
	complexInt(int r, int i) {
		real = r;
		imag = i;
	};
};

struct complexDouble {
	double real;
	double imag;
	complexDouble(double r, double i) {
		real = r;
		imag = i;
	};
};

#endif

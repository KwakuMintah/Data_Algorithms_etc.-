class DoubleMatrix {
public:
    float matOne;
    float matTwo;
        void matrixAddition()
};

DoubleMatrix.matOne = { 1.0, 2.0, 3.0, 4.0 };
DoubleMatrix.matTwo = { 5.0, 6.0, 7.0, 8.0 };

void DoubleMatrix::matrixAddition() {
    int i, j;
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 2; j++) {
            matThree[i][j] = matOne[i][j] + matTwo[i][j];
        };
    };
};
